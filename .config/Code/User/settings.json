{
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.fontFamily": "SF Mono Powerline, Operator Mono Medium, Cascadia Code PL, FontAwesome",
    "git.confirmSync": false,
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "python.jediEnabled": false,
    "liveshare.presence": true,
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "update.mode": "none",
    "telemetry.enableTelemetry": false,
    "todo-tree.tree.showScanModeButton": false,
    "go.useLanguageServer": true,
    "window.menuBarVisibility": "hidden",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 4,
    "extensions.ignoreRecommendations": false,
    "java.semanticHighlighting.enabled": true,
    "java.requirements.JDK11Warning": false,
    "latex.server.autoDownload": true,
    "latex.build.onSave": true,
    "markdown.preview.fontFamily": "Fira Sans",
    "markdown.styles": [
        "~/.config/Code/User/markdown.css"
    ],
    "cSpell.userWords": [
        "footnotemark",
        "footnotetext"
    ],
    "workbench.colorTheme": "Base16 Dark Default"
}